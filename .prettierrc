{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"react",
		"<THIRD_PARTY_MODULES>",
		"/app/",
		"/entities/",
		"/features/",
		"/pages/",
		"/widgets/",
		"/shared/",
		"^../(.*)",
		"^./(.*)",
		"/module.scss/"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
